<form action="/cars/new" method="GET">
	<button type="submit">New car</button>
</form><br>
<form action="/signup" method="GET">
	<button type="submit">Signup</button>
</form><br>
<form action="/login" method="GET">
	<button type="submit">Log In</button>
</form><br>
<form action="/logout" method="GET">
	<button type="submit">Log Out</button>
</form><br>

<h3>Every Car:</h3>
<% Car.all.each_with_index do |car, i| %>
	<blockquote>
		<%= (i+1).to_s + ':  ' %>
    <%= car.brand %>
    <%= car.name %>
    <%= car.make_year %>
  </blockquote>
  <figcaption>&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp-&nbsp<%= car.user.username %></figcaption><br>
<% end %>